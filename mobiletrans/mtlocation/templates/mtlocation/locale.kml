<?xml version="1.0" encoding="UTF-8"?>
<kml xmlns="http://www.opengis.net/kml/2.2">
<Document>
  {% for placemark in placemarks %}<Placemark>
    <name>{{ placemark.name }}</name>
    <description>{{ placemark.description }}</description>
    {{ placemark.point.kml|safe }}
  </Placemark>
  {% endfor %}
</Document>
</kml>